{
  "name": "@jungzl/stylelint-config",
  "version": "0.0.4",
  "description": "Jungzl's stylelint config",
  "author": "Jungzl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "stylelint"
  ],
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "update": "pnpm up -i --latest",
    "lint": "eslint . --fix",
    "release": "bumpp package.json --commit --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "stylelint": "^15.0.0"
  },
  "dependencies": {
    "postcss-html": "^1.5.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-css-modules": "^1.2.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-scss": "^5.0.0"
  },
  "devDependencies": {
    "@jungzl/eslint-config-ts": "^0.0.8",
    "bumpp": "^9.0.0",
    "eslint": "^8.26.0",
    "typescript": "^5.0.0"
  }
}
